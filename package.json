{
  "name": "systemprompt-voice-mcp",
  "private": true,
  "version": "0.3.3",
  "type": "module",
  "scripts": {
    "proxy": "tsx --tsconfig proxy/tsconfig.json proxy/src/index.ts",
    "dev:vite": "vite",
    "dev": "concurrently \"npm run proxy\" \"npm run dev:vite\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run build && concurrently \"npm run proxy\" \"npm run preview\"",
    "test": "vitest run --reporter verbose > test-output.txt",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@iconify/react": "^5.1.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@nextui-org/react": "^2.6.10",
    "@types/react-router-dom": "^5.3.3",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "eventsource": "^3.0.2",
    "express": "^4.21.2",
    "framer-motion": "^11.15.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "next": "^15.1.3",
    "next-themes": "^0.4.4",
    "openapi-client-axios": "^7.5.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "shell-quote": "^1.8.2",
    "spawn-rx": "^5.1.1",
    "usehooks-ts": "^3.1.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/shell-quote": "^1.7.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^1.3.1"
  }
}